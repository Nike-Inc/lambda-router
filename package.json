{
  "name": "lambda-router",
  "version": "3.0.0-0",
  "description": "A router to simplify AWS Lambda Functions.",
  "main": "lib/router.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "test": "jest",
    "test:ci": "run-s check:ci test",
    "test:watch": "jest --watch",
    "build": "run-s build:clean build:tsc",
    "build:clean": "del dist/ ",
    "build:tsc": "tsc --project tsconfig.json",
    "example": "npm run start --prefix example",
    "style": "prettier  --write \"{src,test}/**/*.{ts,tsx}\"",
    "style:ci": "prettier --check \"{src,test}/**/*.{ts,tsx}\"",
    "lint": "eslint \"{src,test}/**/*.{ts,tsx}\" --ext .ts,.tsx",
    "check": "run-s style lint",
    "check:ci": "run-s style:ci lint",
    "size": "run-s build size:check",
    "size:check": "size-limit",
    "release": "np",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/aws-lambda": "^8.10.93",
    "@types/jest": "^27.4.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.11.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "np": "^7.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "size-limit": "^7.0.8",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Nike-Inc/lambda-router.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "router"
  ],
  "author": "Tim Kye",
  "license": "Apache-2",
  "np": {
    "yarn": true
  },
  "size-limit": [
    {
      "path": "lib/router.js",
      "limit": "3.5kb"
    }
  ]
}
